@article{Chu2012,
abstract = {Objectives: The World Health Organization considers Cesarean section rates of 5-15{\%} to be the optimal range for targeted provision of this life saving intervention. However, access to safe Cesarean section in resource-limited settings is much lower, estimated at 1-2{\%} reported in sub-Saharan Africa. This study reports Cesarean sections rates and indications in Democratic Republic of Congo, Burundi, and Sierra Leone, and describe the main parameters associated with maternal and early neonatal mortality. Methods: Women undergoing Cesarean section from August 1 2010 to January 31 2011 were included in this prospective study. Logistic regression was used to model determinants of maternal and early neonatal mortality. Results: 1276 women underwent a Cesarean section, giving a frequency of 6.2{\%} (range 4.1-16.8{\%}). The most common indications were obstructed labor (399, 31{\%}), poor presentation (233, 18{\%}), previous Cesarean section (184, 14{\%}), and fetal distress (128, 10{\%}), uterine rupture (117, 9{\%}) and antepartum hemorrhage (101, 8{\%}). Parity {\textgreater}6 (adjusted odds ratio [aOR] = 8.6, P = 0.015), uterine rupture (aOR = 20.5; P =. 010), antepartum hemorrhage (aOR = 13.1; P =. 045), and pre-eclampsia/eclampsia (aOR = 42.9; P =. 017) were associated with maternal death. Uterine rupture (aOR = 6.6, P{\textless}0.001), anterpartum hemorrhage (aOR = 3.6, P{\textless}0.001), and cord prolapse (aOR = 2.7, P = 0.017) were associated with early neonatal death. Conclusions: This study demonstrates that target Cesarean section rates can be achieved in sub-Saharan Africa. Identifying the common indications for Cesarean section and associations with mortality can target improvements in antenatal services and emergency obstetric care. {\textcopyright} 2012 Chu et al.},
author = {Chu, Kathryn and Cortier, Hilde and Maldonado, Fernando and Mashant, Tshiteng and Ford, Nathan and Trelles, Miguel},
doi = {10.1371/journal.pone.0044484},
file = {::},
issn = {19326203},
journal = {PLoS ONE},
keywords = {Adolescent,Adult,Burundi / epidemiology,Cesarean Section / statistics {\&} numerical data*,Democratic Republic of the Congo / epidemiology,Developing Countries*,Female,Fetal Distress / epidemiology,Fetal Distress / mortality*,Fetal Distress / surgery,Hilde Cortier,Humans,Infant,Infant Mortality,Kathryn Chu,MEDLINE,Maternal Mortality,Miguel Trelles,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Newborn,Obstetric Labor Complications / epidemiology,Obstetric Labor Complications / mortality*,Obstetric Labor Complications / surgery,PMC3433452,Parity,Pre-Eclampsia / epidemiology,Pre-Eclampsia / mortality*,Pre-Eclampsia / surgery,Pregnancy,Prospective Studies,PubMed Abstract,Sierra Leone / epidemiology,Survival Rate,doi:10.1371/journal.pone.0044484,pmid:22962616},
