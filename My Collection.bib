@article{Yaya2018a,
abstract = {Access to safe Cesarean section (C-section) in resource-constrained settings such as sub-Sahara Africa (SSA) region is a foremost approach to reduce maternal mortality. C-section is an obstetric operative procedure used appropriately to improve delivery outcomes. However, errors in the procedure have enormous potential harm that may outweigh the benefits. This study assessed the prevalence and determinants of C-section in several SSA countries. This study examined the prevalence and determinants associated with cesarean delivery in SSA countries. Secondary data of women of reproductive age (15-49 years) from the current Demographic and Health Survey (DHS) in 34 SSA countries was utilized in this study. The mode of delivery among women was the primary outcome variable. Percentage and descriptive statistics were used to conduct univariate analyses. Furthermore, multivariable multilevel logistic regression was used to investigate correlates of C-section among SSA women. Results showed disparities in the percentage of C-section among women from 34 SSA countries. C-section at public healthcare settings ranged from 3{\%} in Burkina Faso to 15.6{\%} in Ghana. However, in private healthcare settings, C-section ranged from 0{\%} in Sao Tome and Principe to 64.2{\%} in Rwanda. Overall, C-section was 7.9{\%} from public healthcare and 12.3{\%} from private healthcare facilities respectively. In the adjusted regression model; women aged 35–49 had increase in the odds of C-section, while a unit increase in the number of children ever born had 17 and 20{\%} significant reduction in the odds of C-section in public and private healthcare respectively. Assessing public healthcare settings; women from richer/richest households, male and large size children at birth had increase in the odds of C-section, while those from rich neighbourhood had reduction in the odds of C-section. In private healthcare settings, women with high decision making power and multiple births had increase in the odds of C-section, while those who attended ANC visits had significant reduction in the odds of C-section. The findings from this study would help formulate health policies and implement actions that would improve the outcome of C-section care. Monitoring of emergency obstetric care services is necessary to address issues connected to poor C-section outcomes in resource-constrained settings. Also training of medical personnel including midwives and nurses in emergency obstetric care, ensuring accessibility to life-saving drugs and supplies should be encouraged in health care system.},
author = {Yaya, Sanni and Uthman, Olalekan A. and Amouzou, Agbessi and Bishwajit, Ghose},
doi = {10.1186/s41256-018-0074-y},
file = {::},
issn = {2397-0642},
journal = {Global Health Research and Policy},
keywords = {Public Health},
month = {dec},
number = {1},
pages = {19},
publisher = {Springer Science and Business Media LLC},
title = {{Disparities in caesarean section prevalence and determinants across sub-Saharan Africa countries}},
url = {https://ghrp.biomedcentral.com/articles/10.1186/s41256-018-0074-y},
volume = {3},
year = {2018}
}
@misc{Dare2019,
abstract = {In many African countries, women giving birth still face substantial risks to their own life and that of their baby. In this issue of The Lancet Global Health, David Bishop and colleagues 1 report on maternal and neonatal outcomes after caesarean section in 22 African countries. Their findings suggest that this risk remains true even for women who have access to caesarean delivery in a hospital facility, the highest level of intervention afforded under emergency obstetric care. The 3792 women who had a caesarean section in 2016 form a nested cohort within the African Surgical Outcomes Study (ASOS)-a large prospective observational study of surgical volume and outcomes on the continent that employed a 7-day snapshot data capture method. 2 Over three-quarters of women having caesarean delivery presented emergently, with a high baseline preoperative risk owing to pregnancy-related complications, including pre-eclampsia or eclampsia, placenta praevia, placental abruption, uterine rupture, and antepartum haemorrhage. The study mortality rate after caesarean delivery was 0{\textperiodcentered}5{\%} (95{\%} CI 0{\textperiodcentered}3-0{\textperiodcentered}8), with a maternal mortality rate of 5{\textperiodcentered}43 per 1000 operations-some 50 times higher than in high-income countries. Preoperative major bleeding risk, perioperative haemorrhage, and anaesthesia-related complications (a composite of failed intubation, aspiration, cardiac arrest, and hypoxia) were independently associated with mortality. One in six women developed a perioperative complication, with postpartum haemorrhage being the most common severe complication. Overall neonatal mortality after caesarean section was 4{\textperiodcentered}4{\%} (3{\textperiodcentered}7-5{\textperiodcentered}0), rising to 18{\textperiodcentered}0{\%} for infants delivered at 35 weeks or less gestational age. The study did not capture the overall number of deliveries at each participating hospital, so it is not possible to ascertain the caesarean section rate. However, in many African countries the reported caesarean section rate remains as low as 2-4{\%}, 3 much lower than the 10-19{\%} rate associated with improved maternal and neonatal outcomes at the population level. 4,5 For the most part, the study cohort were young, had low parity, were healthy before pregnancy, and delivered term infants. As the authors acknowledge, 1 owing to the non-representative sampling strategy used, a disproportionate number of participating centres were university-affiliated hospitals providing regionalised or specialised level care, rather than district hospitals, which act as first-level hospital providers of comprehensive emergency obstetric care for most African women (and typically with fewer resources). 6 Two-thirds of patients were from middle-income countries, where access to maternal and neonatal care, including trained obstetric and anaesthesia providers, is on average much better than in Africa's poorest countries. The true morbidity and mortality after caesarean section on the continent is therefore likely to be higher than reported. One of the major challenges with studying maternal mortality after caesarean section with a 7-day data snapshot method is that death is an infrequent outcome (20 deaths of 3792 caesarean sections), even in countries with high rates by global standards. Pooling deaths across health facilities and countries to achieve stable continental-wide estimates and to identify explanatory variables comes at the expense of the contextual granularity required to understand the root causes of each maternal death within a given facility and country. Both approaches are ultimately needed to document the magnitude of the problem and to identify which deaths and complications after caesarean section occur as a result of poor-quality care or failure to rescue and are attributable to processes occurring within the health facility, and which represent shortcomings in the broader health system. An example of the latter would be prehospital delays that result in presentations too late to significantly change the trajectory of the outcome even in the presence of high-quality emergency obstetric care. Despite these limitations, two areas clearly stand out from the study findings as promising intervention points and are well supported by other analyses. 7,8 The first is the crucial need for safe obstetric anaesthesia in sub-Saharan Africa. In the present study, 1 10{\%} of deaths after caesarean section were attributable to anaesthesia-related complications, which are almost always preventable. The consistent use of surgical safety checklists in the operating room (used in only half of all caesarean sections in the present study) and the availability of pulse oximetry and appropriate monitoring for both spinal and general anaesthesia are low-cost interventions that facilitate early identification See Articles page e513},
author = {Dare, Anna J.},
booktitle = {The Lancet Global Health},
doi = {10.1016/S2214-109X(19)30054-3},
file = {::},
issn = {2214109X},
month = {apr},
number = {4},
pages = {e402--e403},
pmid = {30879499},
publisher = {Elsevier Ltd},
title = {{Making caesarean section safer for African mothers}},
url = {www.thelancet.com/lancetgh},
volume = {7},
year = {2019}
}
@incollection{Byamugisha2020,
abstract = {Caesarean section (CS) refers to delivery of a foetus through surgical incisions made through abdominal and uterine walls. It's a life-saving procedure when complications arise during pregnancy. It may be an emergency or a planned procedure. Although desirable, CS may be medically unnecessary. CS is a major procedure associated with imme diate and long-term maternal and perinatal risks and may have implications for future pregnancies. Since 1985, international healthcare community considers ideal rate for CS to be 10-15{\%}. However, in the last decade, there has been concern about the rising rates of CS from as low as 2{\%} in Africa to as high as 50-60{\%} in Dominican Republic and Latin America. To this effect, there have been attempts to regulate the rates, and the Ten Group Classification System under the Robson criteria is such an attempt. CS rates are on the increase due to varying reasons ranging from patient, institutional, care provider and societal factors. There have been modifications in the CS technique and the drugs used postoperatively from Pitocin to addition of Misoprostol. Need has developed from Reproductive Health Specialists to review indications, rates and terminologies used and evaluate practices in low-, middle-and high-income countries regarding CS.},
author = {Byamugisha, Josaphat and Adroma, Moses},
booktitle = {Recent Advances in Cesarean Delivery},
doi = {10.5772/intechopen.88573},
file = {::},
keywords = {LMICs,caesarean section,rates},
month = {apr},
publisher = {IntechOpen},
title = {{Caesarean Section in Low-, Middle- and High-Income Countries}},
url = {http://dx.doi.org/10.5772/intechopen.88573},
year = {2020}
}
@article{Harrison2016,
abstract = {Cesarean section is an essential maternal healthcare service. Its role in labor and delivery care in low-and middle-income countries is complex; in many low-resource settings it is underutilized in the most needy of populations and overused by the less needy, without clear methods to ensure that universal access is available. Additionally, even if universal access were available, it is not evident that these countries would have the capacity or the finances to appropriate meet demand for the procedure, or that patients would want to utilize the care. This review summarizes the literature and illustrates the complicated relationship that cesarean section, which is rapidly on the rise around the world, has with individuals, communities, and nations in sub-Saharan Africa.},
author = {Harrison, Margo S and Goldenberg, Robert L},
doi = {10.1186/s40748-016-0033-x},
file = {::},
keywords = {Cesarean section,Epidemiology,Low-and middle-income countries,Sub-Saharan Africa Background,Trial of labor after cesarean section,Vaginal birth after cesarean section},
title = {{Cesarean section in sub-Saharan Africa}},
year = {2016}
}
